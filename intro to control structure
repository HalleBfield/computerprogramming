car_color= "Blue"
car_make= "tesla"

if car_color == "Blue" :
    if car_make =="tesla":
        print ("my car is a Blue tesla")
    else:
        print ("My car is Blue")
elif car_color == "Red":
    print('My car is a ferrari')
else:
    print ("i just have a bike")

# for loop
for i in range (1, 10):
    print(i)

sports = ['soccer', 'basketball', 'hockey', 'football']
print (sports)

for item in sports:
    print (item)

i = 1 
while i < 6:
    print ("i'm still going")
    i += 1

for i in range(50):
    if i > 24:
        break 
    else:
        print(i)
    